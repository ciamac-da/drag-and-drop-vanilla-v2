  "bugs": {
    "url": "https://github.com/ciamac-da/drag-and-drop-vanilla-v2/issues"
  },
  "dependencies": {
    "gh-pages": "latest",
    "live-server": "latest",
    "npm-run-all": "latest",
    "sass": "latest"
  }
}
